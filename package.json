  "version": "0.2.0",
  "description": "Ex09: New Viewport adapter",
  "main": "index.js",
  "scripts": {
    "postinstall": "concurrently -c \"yellow,magenta\" -n \"adapter,dapplet\" \"cd adapter && npm i\" \"cd dapplet-feature && npm i\"",

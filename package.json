{
  "name": "example17",
  "version": "0.1.0",
  "description": "Ex.17: Overlay With Login",
  "main": "",
  "scripts": {
    "postinstall": "concurrently -c \"magenta,cyan\" -n \"dapplet,overlay\" \"npm run install:dapplet\" \"npm run install:overlay\"",
    "install:dapplet": "cd dapplet && npm i",
    "install:overlay": "cd overlay && npm i",
    "start": "concurrently -c \"magenta,cyan\" -n \"dapplet,overlay\" \"npm run start:dapplet\" \"npm run start:overlay\"",
    "start:dapplet": "cd dapplet && npm start",
    "start:overlay": "cd overlay && npm start",
    "prettify": "prettier --write ../dapplet-template",
    "eslint": "eslint ../dapplet-template --ext .ts --fix",
    "stylelint": "stylelint ../dapplet-template/**/*.{css,scss,module.scss} --fix"
  },
  "cacheDirectories": [
    "dapplet/node_modules",
    "overlay/node_modules"
  ],
  "author": "Dapplets Project",
  "license": "MIT",
  "devDependencies": {
    "@dapplets/dapplet-extension": "latest",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "rollup": "^2.67.2",
    "rollup-plugin-serve": "^1.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "@types/animejs": "^3.1.5",
    "concurrently": "^6.5.1",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "stylelint": "^14.14.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0"
  }
}

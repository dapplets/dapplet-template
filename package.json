{
  "name": "example4.dapplet-base.eth",
  "version": "0.3.0",
  "description": "Ex04: Overlays",
  "main": "lib/index.js",
  "scripts": {
    "postinstall": "cd overlayWithReact && npm i",
    "start": "npm run start:js",
    "start:react": "concurrently -c \"yellow,blue\" -n \"dapplet,overlay:react\" \"rollup -w --config rollup.config.js\" \"cd overlayWithReact && npm start\"",
    "start:js": "concurrently -c \"yellow,magenta\" -n \"dapplet,overlay:js\" \"rollup -w --config rollup.config.js\" \"cd pure-html-page && npx serve\"",
    "build": "rollup --config rollup.config.js",
    "prettify": "prettier --write ../dapplet-template",
    "eslint": "eslint ../dapplet-template --ext .ts --fix",
    "stylelint": "stylelint ../dapplet-template/**/*.{css,scss,module.scss} --fix"
  },
  "cacheDirectories": [
    "node_modules",
    "overlayWithReact/node_modules"
  ],
  "author": "Dapplets Project",
  "license": "MIT",
  "devDependencies": {
    "@dapplets/dapplet-extension": "latest",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "rollup": "^2.67.2",
    "concurrently": "^6.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "stylelint": "^14.14.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0"
  }
}
